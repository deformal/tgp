{
  "version": "0.1.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": "16.8.0"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "npx tsdx build",
    "test": "npx tsdx test",
    "lint": "npx tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "tgpa",
  "author": "deformal",
  "module": "dist/tgp.esm.js",
  "size-limit": [
    {
      "path": "dist/tgp.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/tgp.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^6.0.4",
    "@types/jest": "^27.0.2",
    "husky": "^7.0.4",
    "size-limit": "^6.0.4",
    "tsdx": "^0.7.2",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  }
}
